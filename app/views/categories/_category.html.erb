<%= link_to category do %>
    <div id="<%= dom_id category %>" class="flex gap-4 flex-1">
      <img src="<%= category.icon %>" alt='Category icon' />
      <div>
          <h3 class='text-zinc-500 text-xl font-bold'>
            <%= category.name %>
          </h3>
          <h3 class='font-bold'>$
          
      </h3>
      </div>
    </div>
<% end %>